{
  "name": "restapi-express-postgresql",
  "version": "0.0.2",
  "engines": {
    "node": ">=16.0.0"
  },
  "description": "API Rest desenvolvida com Express, PostgreSQL e Docker",
  "main": "index.js",
  "scripts": {
    "test": "NODE_ENV=development jest --silent --maxConcurrency=1 --detectOpenHandles --forceExit",
    "start": "node ./dist/server.js",
    "dev": "nodemon src/server.js",
    "build": "sucrase ./src -d ./dist --transforms imports"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tglima/restapi-express-postgresql.git"
  },
  "keywords": [
    "nodejs",
    "docker",
    "express",
    "node",
    "postgresql",
    "node-js",
    "dockerfile",
    "tglima",
    "restapi",
    "rest"
  ],
  "author": "Thiago Lima de Sousa",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tglima/restapi-express-postgresql/issues"
  },
  "homepage": "https://github.com/tglima/restapi-express-postgresql#readme",
  "dependencies": {
    "body-parser": "^1.20.1",
    "cors": "^2.8.5",
    "cpf-check": "^3.0.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "express-rate-limit": "^6.7.0",
    "helmet": "^6.0.1",
    "jsonwebtoken": "^9.0.0",
    "morgan-body": "^2.6.8",
    "pg": "^8.10.0",
    "swagger-ui-express": "^4.6.0",
    "youch": "^3.2.2"
  },
  "devDependencies": {
    "@sucrase/jest-plugin": "^3.0.0",
    "@types/jest": "^29.5.1",
    "@types/supertest": "^2.0.12",
    "eslint": "^8.32.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-security": "^1.6.0",
    "jest": "^29.5.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.3",
    "sucrase": "^3.29.0",
    "supertest": "^6.3.3"
  },
  "nodemonConfig": {
    "execMap": {
      "js": "node -r sucrase/register"
    },
    "ignore": [
      "doc"
    ]
  }
}
